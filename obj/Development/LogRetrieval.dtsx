              <DTS:Property
                DTS:Name="DataBytes">-1</DTS:Property>
            </DTS:Property>
          </DTS:LoggingOptions>
          <DTS:ObjectData>
            <ExpressionTask
              Expression="@[User::LogSql] = &quot;SELECT source, event, message, starttime FROM dbo.sysssislog WHERE message is not null AND LEN(RTRIM(LTRIM(message))) != 0 AND starttime &gt;= (SELECT max(starttime) FROM dbo.sysssislog WHERE message like 'Beginning of package execution%' AND source = '&quot;+ @[System::PackageName]  + &quot;')  ORDER BY starttime&quot;" />
          </DTS:ObjectData>
        </DTS:Executable>
