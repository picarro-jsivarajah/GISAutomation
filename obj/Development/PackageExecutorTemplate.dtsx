		public void Main()
		{
            SmtpClient client = new SmtpClient();
            client.Host = Dts.Variables["$Package::EmailSmtpHost"].Value.ToString();
            client.Port = (int) Dts.Variables["$Package::EmailSmtpPort"].Value;
            client.EnableSsl = true;
            client.Timeout = ((int)Dts.Variables["$Package::EmailTimeout"].Value) * 1000;
            client.DeliveryMethod = SmtpDeliveryMethod.Network;
            client.UseDefaultCredentials = false;
            client.Credentials = new System.Net.NetworkCredential(Dts.Variables["$Package::EmailUsername"].Value.ToString(),
                                                                  Dts.Variables["$Package::EmailPassword"].GetSensitiveValue().ToString());
            try
            {
                MailMessage mail = new MailMessage(Dts.Variables["$Package::EmailFrom"].Value.ToString(),
                                                   Dts.Variables["$Package::EmailTo"].Value.ToString(),
                                                   Dts.Variables["User::EmailSubject"].Value.ToString(),
                                                   Dts.Variables["User::EmailMessage"].Value.ToString());
